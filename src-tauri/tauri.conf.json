{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Support",
  "version": "1.4.0",
  "identifier": "co.customconfig.support",
  "plugins": {
    "store": {}
  },
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Support",
        "width": 450,
        "height": 750,
        "resizable": false,
        "fullscreen": false,
        "decorations": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}